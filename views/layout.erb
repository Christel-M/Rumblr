    <a href="/articles/">My Dashboard</a>
    <a href="/articles/create-article">Create a Post</a>
    <a href="/users/<%=session[:user_id]%>">View Profile</a>
  </div>
<%else%>
  <a class="logo" href="/">
    <img src="https://www.misskatecuttables.com/uploads/shopping_cart/7936/large_cute-puppy.png">
    The Daily Pupper
  </a>
<%end%>

